generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Waitlist {
  id        String   @id @default(cuid())
  email     String   @unique
  source    String?  // UTM source
  referrer  String?  // Full referrer URL
  createdAt DateTime @default(now())
}
